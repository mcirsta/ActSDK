target actions_mips32_2137 {
		alias { "ACTIONS 2137 board with Mips32 4kc processor"}
		packages {
			
		}
		description "
			tactions_mips32_2137 target provides the packages needed to run
			USDK on a MIPS actions board with a MIPS32 4kc processor."
}


target actions_mips32_2135 {
		alias { "ACTIONS 2135 board with Mips32 4kc processor"}
		packages {
		
		}
		description "
			tactions_mips32_2135 target provides the packages needed to run
			USDK on a MIPS actions board with a MIPS32 4kc processor."
}

package CYGPKG_CASE	{
	alias	{"CASE configuration"}
	directory	apps_conf/
	script	case.cdl
}


package	CYGPKG_TOOLS {
	alias {"configuration item for tools"}
	directory	apps_conf/
	script	tools.cdl
}

package CYGPKG_APPS_CONFIG {
	alias {"应用参数配置"}
	directory	apps_conf/
	script apps_config.cdl
	
}


package CYGPKG_NORMAL_CONFIG {
	alias {"常用信息配置"}
	directory	apps_conf/
	script normal_config.cdl
	
}
